{
  "name": "esbuild-plugin-es5",
  "version": "2.1.1",
  "description": "Use the @swc/core transform to convert to ES5 for the esbuild plugin.",
  "keywords": [
    "esbuild-plugin",
    "esbuild",
    "es5"
  ],
  "homepage": "https://github.com/noyobo/esbuild-plugin-es5#readme",
  "bugs": {
    "url": "https://github.com/noyobo/esbuild-plugin-es5/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noyobo/esbuild-plugin-es5.git"
  },
  "license": "MIT",
  "author": {
    "name": "Youbao Nong",
    "email": "noyobo@gmail.com",
    "url": "https://github.com/noyobo"
  },
  "main": "./lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rm -rf ./lib/",
    "lint": "eslint ./src/ --fix",
    "prepare": "npx husky install",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "dependencies": {
    "@swc/core": "^1.11.4",
    "@swc/helpers": "^0.5.15",
    "deepmerge": "^4.3.1"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.37",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "esbuild": "^0.25.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "fs-extra": "^11.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "minimist": "^1.2.8",
    "prettier": "^3.5.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "esbuild": "*"
  },
  "engines": {
    "node": ">=12.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
