{
  "name": "esbuild-plugin-es5",
  "version": "2.1.1",
  "description": "Use the @swc/core transform to convert to ES5 for the esbuild plugin.",
  "keywords": [
    "esbuild-plugin",
    "esbuild",
    "es5"
  ],
  "homepage": "https://github.com/noyobo/esbuild-plugin-es5#readme",
  "bugs": {
    "url": "https://github.com/noyobo/esbuild-plugin-es5/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noyobo/esbuild-plugin-es5.git"
  },
  "license": "MIT",
  "author": {
    "name": "Youbao Nong",
    "email": "noyobo@gmail.com",
    "url": "https://github.com/noyobo"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup src/index.ts --minify --dts --format cjs,esm --out-dir dist",
    "clean": "rm -rf ./dist/",
    "prepare": "npx husky install",
    "test": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@swc/core": "^1.11.5",
    "@swc/helpers": "^0.5.15",
    "deepmerge": "^4.3.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.13.8",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitest/coverage-v8": "3.1.1",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "esbuild": "^0.25.0",
    "fs-extra": "^11.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "minimist": "^1.2.8",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.7"
  },
  "peerDependencies": {
    "esbuild": "*"
  },
  "engines": {
    "node": ">=12.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
